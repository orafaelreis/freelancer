<script>
$('#banner').slides({
	preload: true,
	preloadImage: 'imgs/loading.gif',
	play: 5000,
	pause: 2500,
	hoverPause: true,
	next: 'bt_left',
	prev: 'bt_right'
});

 var hover = false;
$("#banner").mouseenter(function(){
		if(hover)return;
		//$("#banner .bt_left").fadeIn('fast');
		//$("#banner .bt_right").fadeIn('fast');
		$(".caption").fadeIn('fast',function(){hover = true;}).animate({bottom:0},200);
}).mouseleave(function() {
	 if(!hover)return;
	 //$("#banner .bt_left").fadeOut('fast');
	 //$("#banner .bt_right").fadeOut('fast');
	 $(".caption").fadeOut('fast',function(){	hover = false;}).animate({bottom:-35},200);
});
</script>
